<link rel="stylesheet" type="text/css" href="/stylesheets/style.css">
<link rel="stylesheet" type="text/css" href="/stylesheets/tribute.css">
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css"
      integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS" crossorigin="anonymous">

<div class="chat-container" data-author="<%= author %>">
    <div class="contacts-dialog">

        <div class="person-row selected">
            <span class="person">GLOBAL</span><span class="no-of-messages badge badge-danger"></span>
        </div>
    </div>
    <div class="chat-dialog" data-recipient="*">
        <div class="conversation-section"></div>
        <div class="typing-section">
            <div class="message-text" contenteditable="true"> </div>
            <button class="btn btn-info send-message-btn">SEND</button>

        </div>
    </div>
</div>
<script src="/javascript/lib/jquery-3.5.1.min.js" type="text/javascript"></script>
<script src="/javascript/lib/tribute.min.js" type="text/javascript"></script>

<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"
        integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo"
        crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/js/bootstrap.min.js"
        integrity="sha384-B0UglyR+jN6CkvvICOB2joaf5I4l3gm9GU6Hc1og6Ls7i6U/mkkaduKaBhlAXv9k"
        crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.26.0/moment.min.js"></script>
<script src="/javascript/home.js"></script>
